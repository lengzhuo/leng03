<!--文章管理-->
<div role="tabpanel" class="tab-pane fade" id="Section2">
    <div class="pubboxw">
        <div class="manage-search">
            <div class="col-md-8">
                <form>
                    <div class="myselectType">
                        <ul id="main_box">
                            <li class="select_box"><span>全部</span>
                                <ul class="son_ul">
                                    <li>标题</li>
                                    <li>关键字</li>
                                </ul>
                            </li>
                        </ul>
                    </div>
                    <input placeholder="搜索从这里开始..." type="text" id="key">
                    <button type="button" onclick="setKey()"></button>
                </form>
            </div>
            <div class="col-md-4 mt20">
                <div class='checkbox'>
                    <input type='checkbox' id='checkbox1' value="1" onclick="setJing()">
                    <label for='checkbox1'>精选</label>
                </div>
            </div>
        </div>
        <!--文章列表-->
        <ul class="manage-list1" id="articleList"></ul>
        <!--分页-->
        <ul class="pagination my-pagination" id="page"></ul>
    </div>
    <!--编辑文章-->
    <div class="pubbox">
        <form id="uploadForm">
            <div class="row">
                <div class="col-md-3">
                    <div class="form-group">
                        <div class="h4">
                            <span class="glyphicon glyphicon-user"></span>
                            王妞妞
                        </div>
                        <div class="fileinput fileinput-new">
                            <div class="fileinput-new thumbnail" id="preview"></div>
                            <div>
                                <span class="btn btn-primary btn-file my-file">
                                    <span class="fileinput-new">选择文件</span>
                                    <input type="file" onchange="preview(this)"/>
                                </span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-9 my-form">
                    <div class="form-group">
                        <label for="exampleInputPassword1">标题</label>
                        <input class="form-control" placeholder="请输入标题" type="text">
                        <p class="tit-Prompt">请输入标题</p>
                    </div>
                    <div class="mb20">
                        <div class="col-md-6">
                            <label for="exampleInputPassword1">类型</label>
                            <select class="form-control">
                                <option>请选择类型</option>
                                <option>2</option>
                                <option>3</option>
                                <option>4</option>
                                <option>5</option>
                            </select>
                            <p class="tit-Prompt">请选择类型</p>
                        </div>
                        <div class="col-md-6">
                            <label for="exampleInputPassword1">原作者</label>
                            <input class="form-control " placeholder="请输入原作者" type="text">
                            <p class="tit-Prompt">请输入原作者</p>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="exampleInputPassword1">精华文章</label>
                        <div class="col-md-6">
                            <div class="radio-inline">
                                <input class="magic-radio" type="radio" name="radio" id="r1" value="1">
                                <label for="r1">是</label>
                            </div>
                            <div class="radio-inline">
                                <input class="magic-radio" type="radio" name="radio" id="r2" value="0" checked>
                                <label for="r2">否</label>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="row my-content">
                    <div class="form-group">
                        <label><b>文章正文</b> 在这里填写详细的文章内容分享给大家</label>
                        <textarea class="form-control" rows="3"></textarea>
                    </div>
                </div>
                <div class="my-button">
                    <button type="button" class="btn btn-warning "> 立即保存</button>
                    <button type="submit" class="btn btn-danger "> 重新发布</button>
                </div>
            </div>
        </form>
    </div>
</div>